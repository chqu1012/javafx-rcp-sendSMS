<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.collections.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.cell.*?>
<?import javafx.scene.control.cell.PropertyValueFactory?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.GridPane?>

<GridPane prefHeight="600.0" prefWidth="600.0" xmlns:fx="http://javafx.com/fxml">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
  </rowConstraints>
  <TableView fx:id="adbookTableView" GridPane.columnIndex="0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="0" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
    <columns>
      <TableColumn minWidth="100.0" prefWidth="100.0" resizable="true" text="Name">
        <cellValueFactory>
          <PropertyValueFactory property="name" />
        </cellValueFactory>
      </TableColumn>
      <TableColumn minWidth="100.0" prefWidth="100.0" text="Firstname">
        <cellValueFactory>
          <PropertyValueFactory property="firstname" />
        </cellValueFactory>
      </TableColumn>
      <TableColumn minWidth="100.0" prefWidth="200.0" text="Mobile Number">
        <cellValueFactory>
          <PropertyValueFactory property="mobileNumber" />
        </cellValueFactory>
      </TableColumn>
      <TableColumn minWidth="100.0" prefWidth="300.0" text="Email">
        <cellValueFactory>
          <PropertyValueFactory property="email" />
        </cellValueFactory>
      </TableColumn>
    </columns>
  </TableView>
</GridPane>
